{
    "name": "faustbrian/laravel-chartjs",
    "description": "ChartJS Integration with a Fluent Builder for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-ChartJS", "Laravel ChartJS", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\ChartJS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\ChartJS\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\ChartJS\\ChartJSServiceProvider"
            ],
            "aliases": {
                "ChartJS": "BrianFaust\\ChartJS\\Facades\\ChartJS"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
